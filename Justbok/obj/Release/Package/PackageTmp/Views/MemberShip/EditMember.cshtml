@model Justbok.Models.MemberViewModel

@{
    ViewBag.Title = "Edit Member";
}

<!-- Content Wrapper. Contains page content -->
<style>
    #ui-datepicker-div {
        z-index: 10000 !important;
    }

    .required {
        color: #e02222;
        padding-left: 2px;
    }
</style>



<!-- Content Header (Page header) -->
<section class="content-header"></section>

<!-- Main content -->
<section class="content container-fluid">
    <div class="row">
        <div class="col-xs-12">
            <div class="box">
                <div class="box-header">
                    <span class="glyphicon glyphicon-user"></span> <h2 class="box-title" id="memberdetail"> @Html.DisplayFor(model => model.FirstName, new { @class = "form-control" }) @Html.DisplayFor(model => model.LastName, new { @class = "form-control" }) (@Html.DisplayFor(model => model.MemberID, new { @class = "form-control" }))</h2>
                </div>
                <div class="box-body">
                    <div class="row">
                        <div class="col-md-2">
                            <img id="customerimage" class="form-control" alt="Member profile" style="height:100px;width:100px;" src="~/Theme/img/fa-user.png" />

                        </div>
                        <!--Second column-->
                        <div class="col-md-3">
                            <div class="row">
                                <div class="col-md-12">
                                    @{
                                        if (Model.Dob != null)
                                        { 
                                          var today = DateTime.Today;
                                        DateTime dob = Convert.ToDateTime(Model.Dob);
                                        var age = today.Year - dob.Year;
                                        <h4>@age Years old  @Html.DisplayFor(model => model.Gender, new { @class = "form-control" })</h4>
                                        }
                                      

                                    }

                                </div>

                            </div>
                            <!--Second column second row-->
                            <div class="row">
                                <div class="col-md-12">
                                    <h5>Member Since @Html.DisplayFor(model => model.EnrollDate, "{0:dd/MM/yyyy}", new { @class = "form-control" })</h5>
                                </div>

                            </div>
                            <!--Second column second row-->
                        </div>
                        <!--Second column-->
                        <!--Third column-->
                        <div class="col-md-3">
                            <div class="row">
                                <div class="col-md-12">
                                    <h4>@Html.DisplayFor(model => model.MobileNumber, new { @class = "form-control" })</h4>
                                </div>

                            </div>
                            <!--Third column second row-->
                            <div class="row">
                                <div class="col-md-12">
                                    <h5>@Html.DisplayFor(model => model.Email, new { @class = "form-control" })</h5>
                                </div>

                            </div>
                            <!--Third column second row-->
                        </div>
                        <!--Third column-->
                        <!--Fourth column-->
                        <div class="col-md-3">
                            <div class="row">
                                <div class="col-md-12">
                                    <h4><span id="subscriptiontime"></span></h4>
                                </div>

                            </div>
                            <!--Fourth column second row-->
                            <div class="row">
                                <div class="col-md-12">
                                    <span>Expires On: </span>
                                    <span id="expires"></span>
                                </div>

                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <span class="required">Due Amount: </span>
                                    <span id="dueamount" class="required"></span>
                                </div>

                            </div>
                            <!--Fourth column second row-->
                        </div>
                        <!--Fourth column-->

                    </div>
                </div>

            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-xs-12">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <ul class="nav nav-tabs">
                        <li class="active"><a href="#member" data-toggle="tab">Member</a></li>
                        @*<li><a href="#Questionnaire" data-toggle="tab">Questionnaire</a></li>*@
                        <li><a href="#Membership" data-toggle="tab">Membership</a></li>
                        <li><a href="#Measurement" data-toggle="tab">Measurement</a></li>
                        <li><a href="#Workout" data-toggle="tab">Workout</a></li>
                        <li><a href="#Diet" data-toggle="tab">Diet Plan</a></li>
                        <li><a href="#Pictures" data-toggle="tab">Pictures</a></li>
                        <li><a href="#Transfer" data-toggle="tab">Transfer</a></li>
                        <li><a href="#Freeze" data-toggle="tab">Freeze</a></li>
                        <li><a href="#SMS" data-toggle="tab">SMS History</a></li>
                    </ul>
                </div>
                <div class="box-body">
                    <div class="tab-content" style="min-height:575px;">
                        <div class="tab-pane active fade in" id="member">
                            @Html.AntiForgeryToken()
                            @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                            <div class="row">
                                <div class="col-lg-6">
                                    <fieldset>
                                        <legend>Member basic info</legend>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label for="exampleInputEmail1">Member Id</label>
                                                @*<input type="text" id="txtMemberId" class="form-control" placeholder="Member Id" />*@
                                                @Html.TextBoxFor(model => model.MemberID, new { @class = "form-control", placeholder = "Member Id", id = "txtMemberId", @readonly = "readonly" })
                                            </div>
                                            <div class="form-group" style="display:none;">
                                                <label for="exampleInputEmail1">Gym Id</label>
                                                @*<input type="text" id="txtGymid" class="form-control" placeholder="Gym Id" />*@
                                                @Html.TextBoxFor(model => model.GymId, new { @class = "form-control", id = "txtGymid", placeholder = "Gym Id" })
                                                @Html.ValidationMessageFor(model => model.GymId)
                                            </div>

                                            <div class="form-group">
                                                <label for="exampleInputEmail1">First Name</label>
                                                <span class="required" aria-required="true"> * </span>
                                                @*<input type="text" id="txtFirstName" class="form-control" placeholder="First Name" />*@
                                                @Html.TextBoxFor(model => model.FirstName, new { @class = "form-control", id = "txtFirstName", placeholder = "First Name" })
                                                @Html.ValidationMessageFor(model => model.FirstName, "", new { @class = "text-danger" })

                                            </div>
                                            <div class="form-group">
                                                <label>Date of Birth:</label>
                                                <div class="input-group date">
                                                    <div class="input-group-addon">
                                                        <i class="fa fa-calendar"></i>
                                                    </div>
                                                    @{ Html.EnableClientValidation(false); }
                                                    @Html.TextBoxFor(model => model.Dob, "{0:dd/MM/yyyy}", new { @class = "form-control", @type = "datetime", id = "txtDOB" })
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label for="exampleInputEmail1">Address</label>
                                                @*<textarea rows="4" cols="50" id="txtAddress"> </textarea>*@
                                                @Html.TextAreaFor(model => model.MemberAddress, new { @class = "form-control", rows = "3", placeholder = "Enter", id = "txtAddress" })
                                            </div>

                                            <div class="form-group">
                                                <label>Enroll Date:</label>

                                                <div class="input-group date">
                                                    <div class="input-group-addon">
                                                        <i class="fa fa-calendar"></i>
                                                    </div>
                                                    @*<input type="datetime" id="txtEnrollDate" class="form-control" />*@
                                                    @{ Html.EnableClientValidation(false); }
                                                    @Html.TextBoxFor(model => model.EnrollDate, "{0:dd/MM/yyyy}", new { @class = "form-control pull-right", @type = "datetime", id = "txtEnrollDate" })
                                                </div>
                                                <!-- /.input group -->
                                            </div>

                                            @*end of column*@
                                        </div>

                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label for="exampleInputEmail1">Reffered by(Member Id)</label>
                                                <div class="input-group">
                                                    @*<input id="txtReferenceBy" class="form-control"  tabindex="2" disabled="" type="text">*@
                                                    @Html.TextBoxFor(model => model.MemberReference, new { @class = "form-control", id = "txtReferenceBy", disabled = "" })
                                                    <div class="input-group-addon" data-toggle="modal" id="showSelectMemberModal">
                                                        <i class="fa fa-search"></i>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group">

                                                <label for="exampleInputEmail1">Last Name</label>
                                                <span class="required" aria-required="true"> * </span>
                                                @*<input type="text" id="txtLastName" class="form-control" placeholder="Last Name" />*@
                                                @Html.TextBoxFor(model => model.LastName, new { @class = "form-control", id = "txtLastName", placeholder = "Last Name" })
                                                @Html.ValidationMessageFor(model => model.LastName, "", new { @class = "text-danger" })
                                            </div>

                                            <div class="form-group">
                                                <label for="exampleInputEmail1">Email</label>
                                                @*<input type="email"  id="txtEmail" class="form-control" placeholder="Email" />*@
                                                @Html.TextBoxFor(model => model.Email, new { @class = "form-control", placeholder = "Email", id = "txtEmail", type = "email" })
                                            </div>
                                            <div class="form-group">
                                                <label for="exampleInputEmail1">Mobile No.</label>
                                                <span class="required" aria-required="true"> * </span>
                                                @*<input type="number" id="txtMobileno" class="form-control" placeholder="Mobile Number" />*@
                                                @Html.TextBoxFor(model => model.MobileNumber, new { @class = "form-control", placeholder = "Mobile Number", id = "txtMobileno", maxlength = 10 })
                                            </div>
                                            <div class="form-group">
                                                <label for="exampleInputEmail1">Gender</label>
                                                <div class="radio">
                                                    @*<input type="radio" class="form-control" name="gender" value="Male"> Male<br>
                                                        <input type="radio" class="form-control" name="gender" value="Female"> Female*@
                                                    <label>@Html.RadioButtonFor(model => model.Gender, "Female", "Gender") Female</label>
                                                    <label>@Html.RadioButtonFor(model => model.Gender, "Male", "Gender") Male</label>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label for="Branch">Representative</label>
                                                @*<select><option>- Please select -</option></select>*@
                                                @Html.DropDownListFor(model => model.Representative, new SelectList(ViewBag.Stafflist), "- Please select -", new { @class = "form-control", id = "ddlRepresentative" })
                                                <!-- /.input group -->
                                            </div>

                                        </div>
                                    </fieldset>
                                </div>
                                <div class="col-lg-6">
                                    <fieldset>
                                        <legend>Additional info</legend>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                @*<input type="checkbox" class="form-control"  id="chkMarried" value="Yes">*@ 
                                                <label>Married @Html.CheckBoxFor(model => model.Married, new { id = "chkMarried" })</label>
                                            </div>
                                            <div class="form-group">
                                                <label>Spouse Birth Date:</label>
                                                <div class="input-group date">
                                                    <div class="input-group-addon">
                                                        <i class="fa fa-calendar"></i>
                                                    </div>
                                                    @*<input type="datetime" id="txtSpouseBday" class="form-control" />*@
                                                    @Html.TextBoxFor(model => model.SpouseBirthDate, "{0:dd/MM/yyyy}", new { @class = "form-control pull-right", @type = "datetime", id = "txtSpouseBday" })
                                                </div>
                                                <!-- /.input group -->
                                            </div>

                                            <div class="form-group">
                                                <label for="exampleInputEmail1">Occupation</label>
                                                @*<input type="text" id="txtOccupation" class="form-control" placeholder="Occupation" />*@
                                                @Html.TextBoxFor(model => model.Occupation, new { @class = "form-control", placeholder = "Occupation", id = "txtOccupation" })
                                            </div>

                                            <div class="form-group">
                                                <label for="Branch">Source</label>
                                                @if (Model.MemberSource != "Google" && Model.MemberSource != "Reference")
                                                {
                                                    Model.Other = Model.MemberSource;
                                                    Model.MemberSource = "Other";
                                                }
                                                @Html.DropDownListFor(model => model.MemberSource, new SelectList(new List<string>
                                                () { "--Select--", "Google", "Reference", "Other" }), new { @class = "form-control", id = "txtSource" })

                                                @if (Model.MemberSource == "Other")
                                                {
                                                    @Html.TextBoxFor(model => model.Other, new { @class = "form-control", placeholder = "Source", id = "txtOther" })
                                                }
                                                else
                                                {
                                                    @Html.TextBoxFor(model => model.Other, new { @class = "form-control", placeholder = "Source", id = "txtOther", style = "display:none;" })
                                                }


                                            </div>
                                            <div class="form-group">
                                                <label for="exampleInputEmail1">Phone (Office)</label>
                                                @*<input type="text" id="txtPhoneofc" class="form-control" placeholder="Phone(Office)" />*@
                                                @Html.TextBoxFor(model => model.PhoneOffice, new { @class = "form-control", placeholder = "Phone(Office)", id = "txtPhoneofc" })
                                            </div>
                                            <div class="form-group">
                                                <label for="exampleInputEmail1">Programme</label>
                                                @*<input type="text" id="txtProgramme" class="form-control" placeholder="Programme" />*@
                                                @Html.TextBoxFor(model => model.Programme, new { @class = "form-control", placeholder = "Programme", id = "txtProgramme" })
                                            </div>

                                            @*End Div*@
                                        </div>

                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label for="exampleInputEmail1">Spouse Name</label>
                                                @*<input type="text" id="txtSpouseName" class="form-control" placeholder="Spouse Name" />*@
                                                @Html.TextBoxFor(model => model.SpouseName, new { @class = "form-control", placeholder = "Spouse Name", id = "txtSpouseName" })
                                            </div>
                                            <div class="form-group">
                                                <label>Anniversary Date:</label>

                                                <div class="input-group date">
                                                    <div class="input-group-addon">
                                                        <i class="fa fa-calendar"></i>
                                                    </div>
                                                    @*<input type="datetime" id="txtAnniversaryDate" class="form-control" />*@
                                                    @Html.TextBoxFor(model => model.AnniversaryDate, "{0:dd/MM/yyyy}", new { @class = "form-control pull-right", @type = "datetime", id = "txtAnniversaryDate" })
                                                </div>
                                                <!-- /.input group -->
                                            </div>
                                            <div class="form-group">
                                                <label for="exampleInputEmail1">Designation</label>
                                                @*<input type="text" id="txtDesignation" class="form-control" placeholder="Designation" />*@
                                                @Html.TextBoxFor(model => model.Designation, new { @class = "form-control", placeholder = "Designation", id = "txtDesignation" })
                                            </div>
                                            <div class="form-group">
                                                <label for="exampleInputEmail1">Phone (Residence)</label>
                                                @*<input type="text" id="txtPhoneResi" class="form-control" placeholder="Phone(Residence)" />*@
                                                @Html.TextBoxFor(model => model.PhoneResidence, new { @class = "form-control", placeholder = "Phone(Residence)", id = "txtPhoneResi" })
                                            </div>
                                            <div class="form-group">
                                                <label for="exampleInputEmail1">Reffered By</label>
                                                @*<input type="text" id="txtRefferedBy" class="form-control" placeholder="Referred By" />*@
                                                @Html.TextBoxFor(model => model.ReferredBy, new { @class = "form-control", placeholder = "Referred BY", id = "txtRefferedBy" })
                                            </div>

                                            @*End Div*@
                                        </div>
                                    </fieldset>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-6 pull-right text-right ng-scope">
                                    <button type="submit" class="btn btn-default pull-right btn-sm btn-flat">Reset</button>
                                    <button type="submit" class="btn btn-primary pull-right btn-sm btn-flat" style=" margin-right:10px;" id="btnEditSaveMember">Save Member</button>
                                </div>
                            </div>
                        </div>

                        <div class="tab-pane fade in" id="Questionnaire">
                            <h5>Questionnaire</h5>
                            <hr>
                            <div class="col-md-6">
                                <h5>Do you ever faint or get dizzy and lose your balance?</h5>
                                <div class="radio">
                                    <label><input name="optionsRadios" id="optionsRadios1" value="option1" checked="" type="radio"> Yes</label>
                                    <label><input name="optionsRadios" id="optionsRadios1" value="option1" checked="" type="radio"> No</label>
                                </div>

                                <h5>When you perform physical activities,do you feel pain in chest?</h5>
                                <div class="radio">
                                    <label><input name="optionsRadios" id="optionsRadios1" value="option1" checked="" type="radio"> Yes</label>
                                    <label><input name="optionsRadios" id="optionsRadios1" value="option1" checked="" type="radio"> No</label>
                                </div>

                                <h5>When you were not engaging in physical activity,have you experienced chest pain in past month?</h5>
                                <div class="radio">
                                    <label><input name="optionsRadios" id="optionsRadios1" value="option1" checked="" type="radio"> Yes</label>
                                    <label><input name="optionsRadios" id="optionsRadios1" value="option1" checked="" type="radio"> No</label>
                                </div>

                                <h5>Do you have high blood pressure or a heart condition n which a doctor/Medical professional is currently prescribing a medicine?</h5>
                                <div class="radio">
                                    <label><input name="optionsRadios" id="optionsRadios1" value="option1" checked="" type="radio"> Yes</label>
                                    <label><input name="optionsRadios" id="optionsRadios1" value="option1" checked="" type="radio"> No</label>
                                </div>

                                <h5>Do you know of any other reason you should not excercise or increase your physical activity?</h5>
                                <div class="radio">
                                    <label><input name="optionsRadios" id="optionsRadios1" value="option1" checked="" type="radio"> Yes</label>
                                    <label><input name="optionsRadios" id="optionsRadios1" value="option1" checked="" type="radio"> No</label>
                                </div>
                            </div>

                            <div class="col-md-6">
                                <h5>Do you have insulin dependent diabetes?</h5>
                                <div class="radio">
                                    <label><input name="optionsRadios" id="optionsRadios1" value="option1" checked="" type="radio"> Yes</label>
                                    <label><input name="optionsRadios" id="optionsRadios1" value="option1" checked="" type="radio"> No</label>
                                </div>

                                <h5>Has a doctor/Medical professional ever diagnoised you with a heart condition and indicated you should restrict your physical activities?</h5>
                                <div class="radio">
                                    <label><input name="optionsRadios" id="optionsRadios1" value="option1" checked="" type="radio"> Yes</label>
                                    <label><input name="optionsRadios" id="optionsRadios1" value="option1" checked="" type="radio"> No</label>
                                </div>

                                <h5>Do you have injury or orthopeadic condition (such as a back,hip or knee problem) that may worse due to a change in your physical activity?</h5>
                                <div class="radio">
                                    <label><input name="optionsRadios" id="optionsRadios1" value="option1" checked="" type="radio"> Yes</label>
                                    <label><input name="optionsRadios" id="optionsRadios1" value="option1" checked="" type="radio"> No</label>
                                </div>

                                <h5>Are you pregnant?</h5>
                                <div class="radio">
                                    <label><input name="optionsRadios" id="optionsRadios1" value="option1" checked="" type="radio"> Yes</label>
                                    <label><input name="optionsRadios" id="optionsRadios1" value="option1" checked="" type="radio"> No</label>
                                </div>

                                <h5>Are you 69 years of age or older and not used to begin very active?</h5>
                                <div class="radio">
                                    <label><input name="optionsRadios" id="optionsRadios1" value="option1" checked="" type="radio"> Yes</label>
                                    <label><input name="optionsRadios" id="optionsRadios1" value="option1" checked="" type="radio"> No</label>
                                </div>
                            </div>

                        </div>

                        <div class="tab-pane fade in" id="Membership" style="margin-left:0px;margin-right:0px;">
                            <div class="row">
                                <div class="col-lg-12">
                                    <fieldset>
                                        <legend>Membership</legend>
                                        <div style="display:none;">
                                            <label for="Branch">MembershipId</label>
                                            <input type="text" id="txtMembershipid" />
                                            <!-- /.input group -->
                                        </div>

                                        <div class="col-md-6">
                                            <div class="form-group col-md-12">

                                                <label for="Branch">Membership</label>
                                                <span class="required" aria-required="true"> * </span>
                                                @Html.DropDownListFor(model => model.MembershipType, new SelectList(ViewBag.OfferList), "- Please select -", new { @class = "form-control", id = "txtMembershipType" })
                                                <!-- /.input group -->
                                            </div>

                                            <div class="form-group col-md-6">
                                                <label for="exampleInputEmail1">Months</label>
                                                <span class="required" aria-required="true"> * </span>
                                                @Html.TextBoxFor(model => model.Months, new { @class = "form-control", placeholder = "Months", id = "txtMonths" })
                                            </div>
                                            <div class="form-group col-md-6">
                                                <label for="exampleInputEmail1">Amount</label>
                                                <span class="required" aria-required="true"> * </span>
                                                @Html.TextBoxFor(model => model.Amount, new { @class = "form-control", placeholder = "Amount", id = "txtAmount" })
                                            </div>

                                            <div class="form-group col-md-6">
                                                <label>Start Date:</label>
                                                <span class="required" aria-required="true"> * </span>

                                                <div class="input-group date">
                                                    <div class="input-group-addon">
                                                        <i class="fa fa-calendar"></i>
                                                    </div>
                                                    @Html.TextBoxFor(model => model.StartDate, new { @class = "form-control pull-right", @type = "datetime", id = "txtStartDate" })
                                                </div>
                                                <!-- /.input group -->
                                            </div>

                                            <div class="form-group col-md-6">
                                                <label>End Date:</label>
                                                <span class="required" aria-required="true"> * </span>

                                                <div class="input-group date">
                                                    <div class="input-group-addon">
                                                        <i class="fa fa-calendar"></i>
                                                    </div>
                                                    @Html.TextBoxFor(model => model.EndDate, new { @class = "form-control pull-right", @type = "datetime", id = "txtEndate" })
                                                </div>
                                                <!-- /.input group -->
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group col-md-12">
                                                <label for="exampleInputEmail1">Note</label>
                                                @Html.TextAreaFor(model => model.Note, new { @class = "form-control", rows = "3", placeholder = "Enter", id = "txtNote" })
                                            </div>
                                            <div class="box-footer">
                                                <button type="submit" id="btnEnroll" class="btn btn-success pull-right"><i class="fa fa-plus" aria-hidden="true"></i> Enroll Membership</button>
                                            </div>
                                        </div>
                                    </fieldset>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-lg-12">
                                    <fieldset>
                                        <legend>Membership Details</legend>
                                        <div class="table-responsive">
                                            <table id="tblNewMembership" class="table table-bordered table-striped dataTable" role="grid" aria-describedby="example1_info">
                                                <thead>
                                                    <tr role="row">
                                                        <th style="display:none"></th>
                                                        <th>#</th>
                                                        <th>Membership</th>
                                                        <th>Start Date</th>
                                                        <th>End Date</th>
                                                        <th>Month</th>
                                                        <th>Amount</th>
                                                        <th>Due Amount</th>
                                                        <th>Paid Amount</th>
                                                        <th>Status</th>
                                                        <th>Notes</th>
                                                        <th>Action</th>
                                                    </tr>
                                                </thead>
                                                <tbody></tbody>
                                            </table>
                                        </div>
                                    </fieldset>
                                </div>
                            </div>
                        </div>

                        <div class="tab-pane fade in" id="Measurement">
                            <fieldset>
                                <legend>Measurement</legend>
                                <div class="col-lg-12">
                                    <div style="float:left; margin-bottom:5px;">
                                        <table id="output"></table>
                                    </div>
                                    <div style="float:right; margin-bottom:5px;">
                                        <button type="button" class="btn btn-sm btn-flat" id="PrintMeasurement" title="Print"><i class="glyphicon glyphicon-print"></i> Print</button>
                                        <button type="button" class="btn btn-primary btn-sm btn-flat" id="btnSaveNewMeasurements" title="Add"><i class="glyphicon glyphicon-plus"></i> Add</button>
                                    </div>
                                </div>
