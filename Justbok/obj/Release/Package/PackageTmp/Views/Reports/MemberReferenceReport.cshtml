@{
    ViewBag.Title = "Member Reference Report";
}


<div class="" style="">
    <section class="content-header ng-scope">
        <h1>
            Member Reference 
            <div class="pull-right">
                <a onclick="return LoadPage('@Url.Action("Index", "Reports")','Justbok | Reports Membership');" class="btn btn-primary btn-sm btn-flat"><i class="fa fa-arrow-left"></i> Reports</a><br><br>
            </div>
        </h1>
    </section>
    <section class="content ng-scope">
        <div class="row">
            <div class="col-xs-12">
                <div class="box box-primary">
                    <div class="box-header with-border">
                        <h3 class="box-title">Member Reference Report</h3>
                    </div>
                    <div class="box-body">
                        <div id="example1_wrapper" class="dataTables_wrapper form-inline dt-bootstrap">
                            <div class="dataTables_length">
                                <label>
                                    Show
                                    <select id="tblMemberReference_length" name="tblMemberReference_length"
                                            aria-controls="tblMemberReference_length" class="form-control input-sm"
                                            onchange="return ShowChange();">
                                        <option value="10">10</option>
                                        <option value="25">25</option>
                                        <option value="50">50</option>
                                        <option value="100">100</option>
                                    </select> entries
                                </label>
                                <div class="pull-right">
                                    <img src="~/Theme/img/excel.jpg" width="30" height="30" id="dwnldExcel" />
                                    <img src="~/Theme/img/pdf.png" width="30" height="30" id="dwnldPdf" />
                                </div>
                            </div>

                            <div class="col-sm-12 table-responsive">
                                <table id="tblMemberReference" class="table table-bordered table-striped dataTable" role="grid" aria-describedby="example1_info">

                                    <thead>
                                        <tr role="row">
                                        <th class="sorting_disabled col-md-2" rowspan="1" colspan="1" style="width: 184px;">Member Name</th>
                                        <th class="sorting_disabled col-md-10" rowspan="1" colspan="1" style="width: 829px;">
                                            <table style="width:100%">
                                            <thead>
                                                <tr>
                                                <td class="col-md-4">Reference Name</td>
                                                <td class="col-md-2">Joined Date</td>
                                                <td class="col-md-4">Membership</td>
                                                <td class="col-md-2">Amount</td>
                                                </tr></thead>
                                            </table>
                                        </th>
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                                <div class="dataTables_paginate paging_simple_numbers" id="tblMemberReference_paginate">
                                    <ul id="pagination" class="pagination"></ul>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>

            </div>
        </div>

    </section>

</div>

@section Scripts {
    <script src="~/Scripts/Navigation.js?v=1.2"></script>
    <script>

        var GetReportListUrl = '@Url.Action("ReportsMemberReference", "Reports")';
     
        var ExcelUrl = '@Url.Action("GenarateExcelReportsMemberReference", "Reports")';
        var PDFUrl = '@Url.Action("GenaratePdfReportsMemberReference", "Reports")';
        var DownloadExcelUrl = '@Url.Action("Download", "Reports")';
        var DownloadPdfUrl = '@Url.Action("DownloadPDF", "Reports")';

    </script>
<script src="~/Content/Reports/Rpts_MemberReference.js"></script>
    
}
